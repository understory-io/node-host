{
  "name": "@riddance/host",
  "version": "0.0.12",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "riddance",
    "serverless",
    "environment",
    "devenv",
    "opinionated"
